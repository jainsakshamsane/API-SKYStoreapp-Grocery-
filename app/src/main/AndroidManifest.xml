<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@drawable/skystorelogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/skystorelogo"
        android:supportsRtl="true"
        android:theme="@style/Theme.GroceryApp"
        tools:targetApi="31">
        <activity
            android:name=".FlashscreenActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.GroceryApp">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".CartActivity"></activity>
        <activity android:name=".SignupActivity"></activity>
        <activity android:name=".MainActivity"></activity>
        <activity android:name=".PaymentActivity"></activity>
        <activity android:name=".Single_Product_activity"></activity>
        <activity android:name=".OrderstatusActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".ForgotpasswordActivity"></activity>
        <activity android:name=".EditProfileActivity"></activity>
        <activity android:name=".New_Layout"></activity>
        <activity android:name=".Single_Category_acitivity"></activity>
        <activity android:name=".starting_1_activity"></activity>
    </application>

</manifest>